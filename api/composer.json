{
  "type": "raiden",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-bcmath": "*",
    "ext-swoole": "*",
    "adhocore/jwt": "^1.1.3",
    "cesurapp/api-bundle": "^2.1",
    "cesurapp/media-bundle": "^1.1.01",
    "cesurapp/storage-bundle": "^1.1.0",
    "cesurapp/swoole-bundle": "^1.1.03",
    "doctrine/doctrine-bundle": "^2.15",
    "doctrine/orm": "^3.5.0",
    "symfony/console": "^7.3.1",
    "symfony/dotenv": "^7.3",
    "symfony/flex": "^2.8.1",
    "symfony/framework-bundle": "^7.3.1",
    "symfony/lock": "^7.3",
    "symfony/mailer": "^7.3.1",
    "symfony/mime": "^7.3",
    "symfony/notifier": "^7.3",
    "symfony/runtime": "^7.3.1",
    "symfony/security-bundle": "^7.3.1",
    "symfony/translation": "^7.3.1",
    "symfony/twig-bundle": "^7.3.1",
    "symfony/uid": "^7.3.1",
    "symfony/validator": "^7.3.1",
    "symfony/yaml": "^7.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2.7",
    "symfony/browser-kit": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "symfony/maker-bundle": "^1.64",
    "symfony/phpunit-bridge": "^7.3.1",
    "symfony/web-profiler-bundle": "^7.3.1",
    "phpstan/phpstan": "^2.1.20",
    "php-cs-fixer/shim": "^3.84",
    "phpstan/phpstan-doctrine": "^2.0.4",
    "phpstan/phpstan-symfony": "^2.0.7"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\Notifier\\Bridge\\Firebase\\": "packages/Firebase",
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*",
    "symfony/polyfill-intl-idn": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": "@auto-scripts",
    "post-update-cmd": "@auto-scripts",
    "qa:fix": "vendor/bin/php-cs-fixer fix --ansi --allow-risky=yes",
    "qa:lint": "vendor/bin/php-cs-fixer fix --dry-run --diff --ansi --allow-risky=yes",
    "qa:phpstan": "vendor/bin/phpstan analyse --no-progress --ansi --memory-limit 512m",
    "test": "bin/phpunit",
    "test:stop": "bin/phpunit --stop-on-failure --testdox --colors",
    "fix": [
      "@qa:fix",
      "@qa:phpstan",
      "@test"
    ]
  },
  "extra": {
    "symfony": {
      "require": "7.3.*"
    }
  }
}
