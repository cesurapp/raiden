{
  "type": "raiden",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-bcmath": "*",
    "ext-swoole": "*",
    "adhocore/jwt": "^1.1.3",
    "cesurapp/api-bundle": "^2.2.7",
    "cesurapp/media-bundle": "^1.2.1",
    "cesurapp/storage-bundle": "^1.2.1",
    "cesurapp/swoole-bundle": "^1.2.10",
    "doctrine/doctrine-bundle": "^3.2.2",
    "doctrine/orm": "^3.6.1",
    "symfony/console": "^8.0.3",
    "symfony/dotenv": "^8.0",
    "symfony/flex": "^2.10.0",
    "symfony/framework-bundle": "^8.0.3",
    "symfony/lock": "^8.0.3",
    "symfony/mailer": "^8.0.3",
    "symfony/mime": "^8.0",
    "symfony/notifier": "^8.0",
    "symfony/runtime": "^8.0.1",
    "symfony/security-bundle": "^8.0",
    "symfony/translation": "^8.0.3",
    "symfony/twig-bundle": "^8.0.3",
    "symfony/uid": "^8.0",
    "symfony/validator": "^8.0.3",
    "symfony/yaml": "^8.0.1"
  },
  "require-dev": {
    "php-cs-fixer/shim": "^3.92.5",
    "phpstan/phpstan": "^2.1.34",
    "phpstan/phpstan-doctrine": "^2.0.13",
    "phpstan/phpstan-symfony": "^2.0.9",
    "phpunit/phpunit": "^12.5.6",
    "symfony/browser-kit": "^8.0.3",
    "symfony/debug-bundle": "^8.0",
    "symfony/maker-bundle": "^v1.65.1",
    "symfony/web-profiler-bundle": "^8.0.3",
    "zenstruck/foundry": "^2.8.4"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\Notifier\\Bridge\\Firebase\\": "packages/Firebase",
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*",
    "symfony/polyfill-php84": "*",
    "symfony/polyfill-intl-idn": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": "@auto-scripts",
    "post-update-cmd": "@auto-scripts",
    "qa:fix": "vendor/bin/php-cs-fixer fix --ansi --allow-risky=yes",
    "qa:lint": "vendor/bin/php-cs-fixer fix --dry-run --diff --ansi --allow-risky=yes",
    "qa:phpstan": "vendor/bin/phpstan analyse --no-progress --ansi --memory-limit 512m",
    "test": "bin/phpunit",
    "test:debug": "bin/phpunit --display-all-issues",
    "test:stop": "bin/phpunit --stop-on-failure --testdox --colors",
    "fix": [
      "@qa:fix",
      "@qa:phpstan",
      "@test"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "8.0.*"
    }
  }
}
